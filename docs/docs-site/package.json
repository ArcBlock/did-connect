{
  "name": "docs-site",
  "private": true,
  "scripts": {
    "dev": "blocklet dev",
    "dev:child": "blocklet dev --component --app-did=z8iZuvjcXn3Lj1DXsaKkAPvL4y1nf5SSsUsXc --mount-point=docs",
    "start": "xmark start",
    "build": "xmark build",
